{
  "privateEndpoints": {
    "basic-backend": {
      "url": "http://localhost:4000"
    }
  },
  "pipelines": [
    {
      "name": "basic-api",
      "publicEndpoints": [
        { "path": "/" }
      ],
      "processors": [
        {
          "condition": ["always"],
          "action": "log",
          "params": {
            "message": "${req.method} ${req.originalUrl}"
          }
        },
        {
          "condition": ["always"],
          "action": "proxy",
          "params": {
            "privateEndpoint": "basic-backend"
          }
        }
      ]
    }
  ]
}
