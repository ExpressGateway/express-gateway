{
  "name": "@mappedin/express-gateway",
  "version": "3.0.2",
  "description": "A microservices API gateway built on top of ExpressJS",
  "homepage": "https://www.express-gateway.io",
  "repository": "MappedIn/express-gateway",
  "keywords": [
    "microservices",
    "apis",
    "api gateway",
    "rest",
    "express",
    "middleware",
    "endpoints",
    "policies",
    "pipelines",
    "nodejs gateway",
    "oauth2"
  ],
  "contributors": [
    "Roman Lisagor <rlisagor@gmail.com>",
    "Serhii Kuts <sergeykuc@gmail.com>",
    "Irfan Baqui <irfan.baqui@gmail.com>",
    "Kevin Swiber <kswiber@gmail.com>",
    "Al Tsang <agilecto@gmail.com>",
    "Vincenzo Chianese <vincenz.chianese@icloud.com>"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "start": "node lib/index.js",
    "start:dev": "cross-env LOG_LEVEL=debug node lib/index.js",
    "lint": "eslint --fix .",
    "pretest": "eslint .",
    "test": "npm run mocha:istanbul",
    "test:all": "cross-env EG_HTTP_PORT=0 EG_CONFIG_DIR=lib/config EG_DISABLE_CONFIG_WATCH=true mocha test",
    "test:unit": "cross-env EG_HTTP_PORT=0 EG_CONFIG_DIR=lib/config EG_DISABLE_CONFIG_WATCH=true mocha \"./test/{,!(e2e)/**/}*.test.js\"",
    "test:e2e": "mocha test/e2e",
    "mocha:istanbul": "nyc --reporter=lcov npm run test:all && nyc report --report=lcov > coverage.lcov && codecov"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "bin": {
    "eg": "./bin/index.js"
  },
  "nyc": {
    "exclude": [
      "logo",
      "node_modules",
      "coverage",
      "migrations",
      "test"
    ]
  },
  "types": "./index.d.ts",
  "dependencies": {
    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.2",
    "chokidar": "^3.5.1",
    "clone": "^2.1.2",
    "color-convert": "^1.9.3",
    "connect-ensure-login": "0.1.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "express-rate-limit": "^2.14.2",
    "express-session": "^1.17.2",
    "find-up": "^3.0.0",
    "form-urlencoded": "^4.5.1",
    "http-proxy": "^1.18.0",
    "ioredis": "^4.27.2",
    "ioredis-mock": "^4.21.8",
    "js-yaml": "^3.14.1",
    "json-schema-merge-allof": "^0.6.0",
    "json-schema-ref-parser": "^7.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash.flatmap": "^4.5.0",
    "minimatch": "^3.0.4",
    "oauth2orize": "^1.11.0",
    "parent-require": "^1.0.0",
    "proxy-agent": "^5.0.0",
    "rate-limit-redis": "^2.1.0",
    "semver": "^6.3.0",
    "superagent": "^5.3.1",
    "superagent-logger": "^1.1.0",
    "superagent-prefix": "0.0.2",
    "uuid": "^3.4.0",
    "uuid62": "1.0.1",
    "vhost": "3.0.2",
    "winston": "3.2.1",
    "yawn-yaml": "1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/json-schema": "7.0.3",
    "codecov": "^3.8.2",
    "cpr": "^3.0.1",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "find-free-port": "2.0.0",
    "husky": "^3.1.0",
    "istanbul": "0.4.5",
    "lint-staged": "^11.0.1",
    "mocha": "^6.2.3",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "^14.1.1",
    "puppeteer": "^1.20.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "supertest-session": "^4.1.0",
    "tmp": "^0.1.0"
  },
  "yargs": {
    "boolean-negation": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.12.1"
  }
}
